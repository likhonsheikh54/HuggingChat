{
	"version": 2,
	"buildCommand": "bun run build:vercel",
	"installCommand": "bun install",
	"framework": "svelte",
	"outputDirectory": ".vercel/output/static",
	"builds": [
		{
			"src": "package.json",
			"use": "@vercel/static-build"
		}
	],
	"routes": [
		{
			"src": "/(.*)",
			"dest": "/"
		}
	]
}
