{
  "version": 2,
  "framework": "svelte",
  "buildCommand": "npm run vercel-build",
  "outputDirectory": ".svelte-kit/output",
  "installCommand": "npm install", 
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "zeroConfig": true
      }
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
