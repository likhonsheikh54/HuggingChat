{
  "version": 2,
  "buildCommand": "bun run build",
  "installCommand": "bun install",
  "framework": "svelte",
  "outputDirectory": ".vercel/output/static",
  "builds": [
    {
      "src": "build/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
